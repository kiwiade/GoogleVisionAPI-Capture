<Window x:Class="GoogleCloudVisionTest.ZoomImage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GoogleCloudVisionTest"
        mc:Ignorable="d"
        Title="ZoomImage" Height="450" Width="800">
    <Grid>
        <Image x:Name="image" Stretch="Fill" MouseDown="image_MouseDown" MouseEnter="image_MouseEnter" MouseLeave="image_MouseLeave"  ></Image>
    </Grid>
</Window>
